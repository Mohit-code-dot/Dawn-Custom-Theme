{% schema %}
  {
    "name": "TimeLine",
    "max_blocks": 7,
    "settings": [
      {
        "type": "text",
        "id": "timelinetext",
        "label": "Add Heading"
      },
      {
        "type": "text",
        "id": "timelinebtntext",
        "label": "Button Text"
      },
      {
        "type": "url",
        "id": "timelinebtnurl",
        "label": "Button Url"
      }
    ],
    "blocks": [
      {
        "type": "text",
        "name": "Add Block",
        "settings": [
