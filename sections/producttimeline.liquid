    right: -5%;
    width:80px;
    height:80px;
    border-radius: 50%;
    background:url("{{ section.settings.timeimg07 | img_url:'master' }}");
    background-position:center;
    background-size:cover;
  }
  #timelinecenterimg{
    position: absolute;
    top: 60%;
    left: 50%;
    height: 400px;
    width: 400px;
    transform: translate(-50%, -50%);
  }
  #timelinecenterimg img{
     border-radius: 50%;
     object-fit:cover;
  }
  .section-timeline a {
    background: #000;
    padding: 0.5rem 2rem;
    border-radius: 10px;
    color:#fff;
    text-decoration:none;
}
  @media(max-width:767px){
    .section-timeline{
      display:none;
    }
  }
</style>
<section class="section-timeline">
  <h2 style="margin-top:2rem;">{{ section.settings.timelinetext }}</h2>
  <a href="{{ section.settings.timelinebtnurl }}">{{ section.settings.timelinebtntext }}</a>
<div id="timelinewrapper">
<div id="timeline">
  {% for block in section.blocks %} 
    <style>
      #time01{{ block.id }} img{
        height:80px;
        width:80px;
        border-radius:50%;
        object-fit:cover;
      }
      #timelinecenter{
        height:400px;
        object-fit:cover;
      }
    </style>
  <div id="time01{{ block.id }}">
    <img src="{{ block.settings.timeimg | img_url:'master' }}" width="" height="">
  </div>
    {% endfor %}
</div>
  <div id="timelinecenterimg">
