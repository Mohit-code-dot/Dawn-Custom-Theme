<script>
  var swiper1 = new Swiper(".mySwiper1", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
        1920: {
          slidesPerView: 5,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
  var swiper2 = new Swiper(".mySwiper2", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
  var swiper3 = new Swiper(".mySwiper3", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
  var swiper4 = new Swiper(".mySwiper4", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
  var swiper5 = new Swiper(".mySwiper5", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });
  var swiper6 = new Swiper(".mySwiper6", {
      slidesPerView: 1,
      spaceBetween: 0,
      loop:true,
    breakpoints: {
        640: {
          slidesPerView: 1,
          spaceBetween: 20,
        },
        768: {
          slidesPerView: 1,
          spaceBetween: 40,
        },
        1024: {
          slidesPerView: 4,
          spaceBetween: 50,
        },
      },
    navigation: {
        nextEl: ".swiper-button-next",
        prevEl: ".swiper-button-prev",
      },
    });

    var select = document.querySelector(" #collectioncardsection .select");
    var options_list = document.querySelector(" #collectioncardsection .options-list");
    var options = document.querySelectorAll(" #collectioncardsection .option");
    var selected  = document.querySelector(" #collectioncardsection .selected ");
    var span = document.querySelector(" #collectioncardsection .select span");
    var tag01 = document.querySelector("#tag01");
    var tag02 = document.querySelector("#tag02");
    var tag03 = document.querySelector("#tag03");
    var tag04 = document.querySelector("#tag04");
    var tag05 = document.querySelector("#tag05");
    var swiper0 = document.querySelector("#swiper0");
    var swiper01 = document.querySelector("#swiper01");
    var swiper02 = document.querySelector("#swiper02");
    var swiper03 = document.querySelector("#swiper03");
    var swiper04 = document.querySelector("#swiper04");
    var swiper05 = document.querySelector("#swiper05");


    //show & hide options list
    select.addEventListener("click", () => {
      options_list.classList.toggle("active");
    });

    //select option
    options.forEach((option) => {
      option.addEventListener("click", () => {
        options.forEach((option) => { option.classList.remove('selected') });
        select.querySelector("span").innerHTML = option.innerHTML;
        option.classList.add("selected");
        options_list.classList.toggle("active");
        swiper0.classList.add("alwayshide");
        var ttttt = span.innerHTML;
       if( ttttt == tag01.innerHTML ){
          swiper05.classList.remove("showhide");
          swiper04.classList.remove("showhide");
          swiper03.classList.remove("showhide");
          swiper02.classList.remove("showhide");
          swiper01.classList.add("showhide");
       }else if(ttttt == tag02.innerHTML){
          swiper05.classList.remove("showhide");
          swiper04.classList.remove("showhide");
          swiper03.classList.remove("showhide");
          swiper01.classList.remove("showhide");
          swiper02.classList.add("showhide");
       }
       else if( ttttt == tag03.innerHTML ){
          swiper04.classList.remove("showhide");
